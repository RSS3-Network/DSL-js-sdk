{
  "name": "@rss3/js-sdk",
  "description": "RSS3 JavaScript SDK, the TurbochargerðŸŒªï¸ for Your Next Open Web Development.",
  "version": "0.6.27",
  "author": "Natural Selection Labs and RSS3 Contributors",
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=v19.8.1"
  },
  "scripts": {
    "generate": "npm run build && DEFAULT_RSS3_NET='https://api.dev.rss3.io' babel-node --presets 'es2015' lib/types/generate",
    "build": "rm -rf lib dist && tsc && webpack",
    "watch-ts": "tsc -w",
    "watch-js": "webpack -w",
    "watch": "concurrently npm:watch-ts npm:watch-js",
    "test": "vitest run src",
    "lint": "cspell --no-progress '**' && eslint --max-warnings=0 src && prettier -c . && tsc --noEmit",
    "format": "eslint --fix && prettier -w .",
    "prepare": "npm run build",
    "pub": "npm version patch --force && npm publish",
    "update": "npx vitest run -u src"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/node": "^20.3.2",
    "@types/qs": "^6.9.8",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^8.2.1",
    "cspell": "^6.31.1",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4",
    "vitest": "^1.0.0-beta.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "openapi-fetch": "^0.7.10",
    "qs": "^6.11.2"
  },
  "files": [
    "lib",
    "!lib/**/*.test.{d.ts,js}",
    "README.md",
    "LICENSE",
    "umd"
  ]
}
