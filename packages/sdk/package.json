{
	"name": "@rss3/sdk",
	"version": "0.0.21",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		}
	},
	"files": ["dist", "src"],
	"sideEffects": false,
	"scripts": {
		"build": "tsup --dts",
		"dev": "tsup --watch"
	},
	"dependencies": {
		"@rss3/api-core": "workspace:*",
		"@rss3/api-utils": "workspace:*"
	}
}
